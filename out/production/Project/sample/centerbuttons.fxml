<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="435.0" prefWidth="473.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SecondController">
   <children>
      <HBox layoutY="6.0" prefHeight="423.0" prefWidth="473.0">
         <children>
            <VBox prefHeight="368.0" prefWidth="188.0">
               <children>
                  <Region prefHeight="80.0" prefWidth="201.0" />
                  <Button fx:id="btnAddToCart" mnemonicParsing="false" onAction="#btnAddToCartClick" text="Dodaj do koszyka">
                     <VBox.margin>
                        <Insets left="45.0" />
                     </VBox.margin>
                  </Button>
                  <Region prefHeight="60.0" prefWidth="201.0" />
                  <Button fx:id="btnDeleteFromCart" mnemonicParsing="false" onAction="#btnDeleteFromCartClick" text="Usuń z koszyka">
                     <VBox.margin>
                        <Insets left="50.0" />
                     </VBox.margin>
                  </Button>
                  <Region prefHeight="60.0" prefWidth="201.0" />
                  <Button fx:id="btnClearCart" mnemonicParsing="false" onAction="#btnClearCartClick" text="Wyczyść koszyk">
                     <VBox.margin>
                        <Insets left="50.0" />
                     </VBox.margin>
                  </Button>
                  <Region prefHeight="60.0" prefWidth="201.0" />
                  <Button fx:id="btnExport" mnemonicParsing="false" onAction="#btnExportClick" text="Eksport do PDF">
                     <VBox.margin>
                        <Insets left="50.0" />
                     </VBox.margin>
                  </Button>
                  <Region prefHeight="80.0" prefWidth="201.0" />
               </children>
            </VBox>
            <Region prefHeight="429.0" prefWidth="31.0" />
            <TableView fx:id="tableCart" prefHeight="429.0" prefWidth="292.0">
              <columns>
                <TableColumn fx:id="columnCartName" prefWidth="155.0" text="Nazwa" />
                <TableColumn fx:id="columnCartPieces" prefWidth="64.0" text="Sztuki" />
                  <TableColumn fx:id="columnCartPrice" prefWidth="60.0" text="Cena" />
              </columns>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>
